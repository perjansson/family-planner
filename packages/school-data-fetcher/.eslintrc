{
  "env": { "node": true, "es6": true },
  "parserOptions": { "ecmaVersion": 8 },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "import"],
  "rules": { "no-console": [0] },
  "settings": {
    "import/resolver": {
      "node": { "moduleDirectory": ["./node_modules", "./"] }
    }
  }
}
